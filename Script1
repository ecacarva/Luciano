# Separando o conjunto de dados por dominio
table(dados$Domain)
quali<-dados[Domain == "Quality of Life",]
fisico<-dados[Domain == "Physical",]
psico<-dados[Domain == "Psych",]
social<-dados[Domain == "Social",]
ambiente<-dados[Domain == "Environment",]
